///Config///
var tab = "https://google.com"//Tab which is spam opened.
var speed = "0" //Speed at which tabs are opened (in milliseconds, 0 = instantly)

///Script (do not edit)///

function spamTabs(){window.open(tab);setTimeout(spamTabs,speed)}spamTabs();
